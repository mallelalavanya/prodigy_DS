   "source": [
    "# Plot the distribution of sentiment\n",
    "plt.figure(figsize=(10, 6))\n",
    "sns.countplot(x='sentiment', data=data, palette='viridis')\n",
    "plt.title('Sentiment Distribution')\n",
    "plt.xlabel('Sentiment')\n",
    "plt.ylabel('Count')\n",
    "plt.show()\n",
    "\n",
    "# Group by 'entity' and 'sentiment' to analyze sentiment for different entities\n",
    "entity_sentiment = data.groupby(['entity', 'sentiment']).size().unstack().fillna(0)\n",
    "\n",
    "# Plot the sentiment for each entity\n",
    "entity_sentiment.plot(kind='bar', stacked=True, figsize=(14, 7))\n",
    "plt.title('Sentiment by Entity')\n",
    "plt.xlabel('Entity')\n",
    "plt.ylabel('Count')\n",
    "plt.legend(title='Sentiment',bbox_to_anchor=(1.25,0.5))\n",
    "plt.show()\n"
   ]
